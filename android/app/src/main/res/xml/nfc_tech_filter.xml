<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- NfcA: Most common NFC tag type (NTAG, MIFARE, etc.) -->
    <tech-list>
        <tech>android.nfc.tech.NfcA</tech>
    </tech-list>
    <!-- NfcB: ISO 14443-3B tags -->
    <tech-list>
        <tech>android.nfc.tech.NfcB</tech>
    </tech-list>
    <!-- NfcF: FeliCa tags (common in Japan) -->
    <tech-list>
        <tech>android.nfc.tech.NfcF</tech>
    </tech-list>
    <!-- NfcV: ISO 15693 vicinity tags -->
    <tech-list>
        <tech>android.nfc.tech.NfcV</tech>
    </tech-list>
    <!-- Ndef: Tags with NDEF data -->
    <tech-list>
        <tech>android.nfc.tech.Ndef</tech>
    </tech-list>
    <!-- NdefFormatable: Tags that can be formatted as NDEF -->
    <tech-list>
        <tech>android.nfc.tech.NdefFormatable</tech>
    </tech-list>
    <!-- MifareClassic: MIFARE Classic tags -->
    <tech-list>
        <tech>android.nfc.tech.MifareClassic</tech>
    </tech-list>
    <!-- MifareUltralight: MIFARE Ultralight tags (including NTAG) -->
    <tech-list>
        <tech>android.nfc.tech.MifareUltralight</tech>
    </tech-list>
    <!-- IsoDep: ISO-DEP (ISO 14443-4) tags -->
    <tech-list>
        <tech>android.nfc.tech.IsoDep</tech>
    </tech-list>
</resources>
